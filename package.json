{
  "name": "Ahoy-dev",
  "version": "1.4.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:windows": "node scripts/windows-dev.js",
    "build": "npm run typecheck && electron-vite build",
    "build:fast": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win-fast": "npm run build:fast && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:mac-universal": "electron-vite build && electron-builder --mac --universal",
    "build:mac-dev:test": "node scripts/build-specific.js mac-dev test",
    "build:mac-dev:prod": "node scripts/build-specific.js mac-dev prod",
    "build:mac-intel:test": "node scripts/build-specific.js mac-intel test",
    "build:mac-intel:prod": "node scripts/build-specific.js mac-intel prod",
    "build:win:test": "node scripts/build-specific.js win test",
    "build:win:prod": "node scripts/build-specific.js win prod",
    "build:mac-dev": "npm run build:mac-dev:test",
    "build:mac-intel": "npm run build:mac-intel:prod",
    "build:win": "npm run build:win:prod",
    "build:mac-intel-force": "npm run clean:deps && npm run install:x64 && npm run build:mac-intel:prod",
    "build:mac-arm": "electron-vite build && electron-builder --mac --arm64",
    "build:linux": "electron-vite build && electron-builder --linux",
    "setup-cache": "node setup-cache.js",
    "build:mac-cached": "npm run setup-cache && npm run build:mac-dev:test",
    "build:mac-safe": "node scripts/build-mac-safe.js",
    "build:dev": "node scripts/build-dev.js",
    "build:dev:mac": "node scripts/build-dev.js mac",
    "build:dev:win": "node scripts/build-dev.js win",
    "build:dev:linux": "node scripts/build-dev.js linux",
    "build:prod": "node scripts/build-prod.js",
    "build:prod:mac": "node scripts/build-prod.js mac",
    "build:prod:win": "node scripts/build-prod.js win",
    "build:prod:linux": "node scripts/build-prod.js linux",
    "test:build-config": "node scripts/test-build-config.js",
    "test:build-config:test": "node scripts/test-build-config.js test",
    "test:build-config:prod": "node scripts/test-build-config.js prod",
    "verify:build": "node scripts/verify-build-config.js",
    "test:shortcuts": "node scripts/test-shortcuts.js",
    "clean:build": "node scripts/clean-build.js",
    "switch-server": "node scripts/switch-server.js",
    "prune:prod": "npm prune --omit=dev",
    "server:test-a": "node scripts/switch-server.js test-a",
    "server:test-b": "node scripts/switch-server.js test-b",
    "server:local": "node scripts/switch-server.js local",
    "server:staging": "node scripts/switch-server.js staging",
    "server:custom": "node scripts/switch-server.js custom",
    "server:current": "node scripts/switch-server.js current",
    "server:list": "node scripts/switch-server.js list",
    "test-config": "node scripts/test-config.js",
    "setup:macos-signing": "node scripts/setup-macos-signing.js",
    "clean:deps": "rm -rf node_modules && rm package-lock.json",
    "install:x64": "npm install --target_arch=x64 --target_platform=darwin --cache /tmp/.npm",
    "install:arm64": "npm install --target_arch=arm64 --target_platform=darwin",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --output-dir storybook-static"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.24.0",
    "@ant-design/cssinjs-utils": "^1.1.3",
    "@ant-design/icons": "^6.0.0",
    "@aws-sdk/client-s3": "^3.850.0",
    "@aws-sdk/s3-request-presigner": "^3.850.0",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@tanstack/react-query": "^5.80.6",
    "@types/better-sqlite3": "^7.6.13",
    "antd": "^5.25.4",
    "axios": "^1.10.0",
    "better-sqlite3": "^12.0.0",
    "dayjs": "^1.11.13",
    "node-machine-id": "^1.1.12",
    "qrcode.react": "^4.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.2",
    "remark-gfm": "^4.0.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^22.14.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "less": "^4.3.0",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "rollup-plugin-visualizer": "^5.14.0",
    "ws": "^8.18.2",
    "vitest": "^2.1.0",
    "jsdom": "^24.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/user-event": "^14.5.0",
    "storybook": "^8.2.0",
    "@storybook/addon-essentials": "^8.2.0",
    "@storybook/addon-interactions": "^8.2.0",
    "@storybook/addon-links": "^8.2.0",
    "@storybook/react-webpack5": "^8.2.0",
    "@storybook/builder-webpack5": "^8.2.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.2.1",
    "@babel/core": "^7.25.2",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "less-loader": "^12.2.0",
    "style-loader": "^3.3.4",
    "css-loader": "^6.11.0"
  },
  "overrides": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@ant-design/cssinjs": "^1.24.0",
    "@ant-design/cssinjs-utils": "^1.1.3"
  },
  "resolutions": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
